! non-color settings

XTerm*transparent: false
XTerm*termName: xterm-256color
urxvt*geometry: 160x50
urxvt*shading: 20
urxvt*saveLines: 4500
urxvt*scrollBar: false
urxvt*letterSpace: 0
urxvt*cursorBlink: false
urxvt*urlLauncher: firefox

xterm*metaSendsEscape: true

! fonts settings
URxvt.font: -fontforge-monaco-medium-r-normal--12-120-72-72-p-59-iso10646-1
URxvt.boldFont: -fontforge-monaco-bold-r-normal--12-120-72-72-p-59-iso10646-1
!urxvt*font: xft:Terminus:size=10
xterm*faceName: xft:Terminus:size=10

! rofi settings

rofi.background: #073642
rofi.foreground: #eee8d5
rofi.highlightbg: #b58900
rofi.highlightfg: #ffffff
rofi.bordercolor: #678698
rofi.padding: 1
rofi.lines: 10
rofi.borderwidth: 1
rofi.opacity: 100
rofi.font: Inconsolata 10
rofi.location: 7
rofi.yoffset: 0
rofi.xoffset: 0
rofi.width: 100
rofi.fixed_num_lines: 1
rofi.hmode: false
rofi.terminal: st
rofi.ssh-set-title: false
    
! end

